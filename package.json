{
  "name": "test-automation-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "generar-reporte": "allure generate allure-results",
    "limpiar-reporte": "rimraf ./allure-report && rimraf ./allure-results",
    "browser:chrome": "npx cypress run --browser=chrome --headed --spec cypress/e2e/login.cy.js --reporter mocha-allure-reporter",
    "ejecutar-pruebas": "--headed --env allure=true",
    "abrir-reporte": "npx allure open allure-report",
    "test": "npm run limpiar-reporte && npx cypress run ejecutar-pruebas && npm run generar-reporte && abrir-reporte",
    "test1": "npm run limpiar-reporte && npx cypress run ejecutar-pruebas"
  },
