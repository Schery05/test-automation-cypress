  "devDependencies": {
    "allure-commandline": "^2.32.0",
    "allure-cypress": "^3.0.7",
    "cypress": "^13.16.1",
    "mocha-allure-reporter": "^1.4.0"
  },
  "dependencies": {
    "@mmisty/cypress-allure-adapter": "^3.2.2",
    "rimraf": "^6.0.1"
  }
